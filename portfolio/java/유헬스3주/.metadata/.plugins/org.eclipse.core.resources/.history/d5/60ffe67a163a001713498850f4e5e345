class tangle {
	int w ,h;
	public int area(){
		return w*h;
	}
	public int perimeter(){
		return (w*2)*(h*2);
	}
	public String toSring(){
		return "넓이 :"+area()+"둘레 :"+perimeter();
	}
}
public class 연습11 {
	public static void main (String [] args){
		tangle obj = new tangle();
		obj.w=2;
		obj.h=3;
		System.out.print( obj.toString() );
	}
}
