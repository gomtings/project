public class food {
	private int cal;
	private int cost;
	private int kg;

	public int getcal() {
		return cal;
	}

	public void setcal(int cal) {
		this.cal = cal;
	}

	public int getcost() {
		return cost;
	}

	public void setcost(int cost) {
		this.cost = cost;
	}

	public int getkg() {
		return kg;
	}

	public void setkg(int kg) {
		this.kg = kg;
	}

}
