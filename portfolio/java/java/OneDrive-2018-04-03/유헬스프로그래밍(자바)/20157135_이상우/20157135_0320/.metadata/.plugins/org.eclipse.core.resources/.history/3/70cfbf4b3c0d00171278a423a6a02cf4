import java.util.Scanner; 
public class test4 {
	public static void main(String[] args){
		int r;
		int c2;
		int c3;
		int c4;
		final double PI =3.14;
		
		Scanner t5 = new Scanner(System.in); 
		System.out.print("구의 반지름은:");
		r =t5.nextInt();
		c2 =  PI * r * r; //넓이
		c3 = 2 * PI * r; // 둘레
		c4 = 3/4*PI*r*r*r;//부피
	}
}
